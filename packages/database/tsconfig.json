  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "outDir": "lib",
    "strict": true,
    "target": "esnext",
  },
  "compileOnSave": true,
  "include": ["src"],
}
